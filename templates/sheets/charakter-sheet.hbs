			</div>

	    <div class="fillerHead"></div>

		<section class="sheet-body">

			<div id="mainPage" class="tab mainPage primary" data-group="primary" data-tab="mainPage">
				{{> "systems/gdsa/templates/partials/character-sheet-mainPage.hbs"}}
			</div>

			<div id="skillPage" class="tab skillPage primary" data-group="primary" data-tab="skillPage">
				{{> "systems/gdsa/templates/partials/character-sheet-skillPage.hbs"}}
			</div>

			<div id="combatePage" class="tab combatePage primary" data-group="primary" data-tab="combatePage">
				{{> "systems/gdsa/templates/partials/character-sheet-combatPage.hbs"}}
			</div>

			<div id="gmPage" class="tab gmPage primary" data-group="primary" data-tab="gmPage">
				{{> "systems/gdsa/templates/partials/character-sheet-gmPage.hbs"}}
			</div>

			<div id="magicPage" class="tab magicPage primary" data-group="primary" data-tab="magicPage">
				{{> "systems/gdsa/templates/partials/character-sheet-magicPage.hbs"}}
			</div>

			<div id="holyPage" class="tab holyPage primary" data-group="primary" data-tab="holyPage">
				{{> "systems/gdsa/templates/partials/character-sheet-holyPage.hbs"}}
			</div>

			<div id="itemPage" class="tab itemPage primary" data-group="primary" data-tab="itemPage">
				{{> "systems/gdsa/templates/partials/character-sheet-itemPage.hbs"}}
