			<div id="holytraitPage" class="tab traitPage primary" data-group="primary" data-tab="holytraitPage">
				{{> "systems/gdsa/templates/partials/character-sheet-holytraitPage.hbs"}}
			</div>

			<div id="characterView" class="tab traitPage primary" data-group="primary" data-tab="characterView">
				{{> "systems/gdsa/templates/partials/charakter-sheet-view.hbs"}}
