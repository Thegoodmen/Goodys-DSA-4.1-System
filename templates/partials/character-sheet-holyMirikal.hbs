<div class="wonderBox">
    <div class="menuLine">
        {{localize "GDSA.charactersheet.holyTemp"}}
        <select class="SelHelper1 changeMirTemp" name="system.mirTemp">{{selectOptions template.cults.all selected=system.mirTemp valueAttr="id" labelAttr="name"}}</select>
