            <div class="traithrbox3"><hr class="traithrMain2" /></div>
            {{localize "GDSA.charactersheet.attributes"}}
            <div class="traithrbox3"><hr class="traithrMain2" /></div>
        </div>
        <div class="skillFiller"></div>
        {{#each config.attributes as |name type|}}
            <div class="mirSkillBox">
                <div class="mirSkillName">{{localize (concat "GDSA.charactersheet." name "")}}</div>
                <div class="mirSkillSelc">
                    <select class="SelHelper2" name="system.mirikal.cus.{{localize (concat "GDSA.charactersheet." name "")}}">
                        {{selectOptions ../config.mirDis selected=(getData ../system/mirikal/cus (localize (concat "GDSA.charactersheet." name ""))) blank="" localize=true}}
                    </select>
                </div>
                <a class="item mirRoll" title="{{localize 'GDSA.chat.skill.roll'}}" data-name="{{localize (concat "GDSA.charactersheet." name "")}}"><i class="fas fa-dice-d20 faD20Stat"></i></a>
            </div>
        {{/each}}

